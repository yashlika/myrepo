pipeline {
    agent any 
    stages {
        stage('pull') { 
            steps {
                 git credentialsId: 'github', url: 'https://github.com/yashlika/myrepo.git'
            }
        }
        stage('build') { 
            steps {
                 sh 'mvn clean package'
            }
        }
        stage('test') { 
            steps {
                // 
            }
        }
    }
}
